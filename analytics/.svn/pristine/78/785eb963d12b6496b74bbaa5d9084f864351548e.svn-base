<head>
	<!--Noise specific includes go here to avoid cluttering DAWN's index.html-->
	<link rel="stylesheet" href="stylesheets/BigBrother-style.css">
</head>
<div ng-include src ="'views/DAWN/topBar.html'" ng-controller="topCtrl"></div>

<body >
<div class="container-fluid">
	<div class="well well-md">
		<div class="row-fluid">
			<div class="row-md-height">

						<ul class="nav nav-tabs">
							<li>
								<div class="dropdown">
									<button class="btn btn-primary" type="button" id="menu1" data-toggle="dropdown"> {{selectTool}}
										<span class="caret"></span>

									</button>
									<ul class="dropdown-menu scrollable-menu" role="menu" aria-labelledby="menu1"> 
										<li ng-repeat="tool in tools">
											<a tabindex="-1" href="#" ng-click="setMenuSelection($event, 'Tool' ,tool)" >{{tool}}</a>
										</li>
									</ul>
								</div>
							</li>
							<li>
								<div class="dropdown">
									<button class="btn btn-primary" type="button" id="menu1" data-toggle="dropdown">{{selectRelease}}
										<span class="caret"></span>
									</button>
									<ul class="dropdown-menu  scrollable-menu" role="menu" aria-labelledby="menu1"> 
										<li ng-repeat="release in releases">
											<a tabindex="-1" href="#" ng-click="setMenuSelection($event, 'Release', release)" >{{release}}</a>
										</li>
									</ul>
								</div>								 
							</li>
							<li>
								<div class="dropdown">
									<button class="btn btn-primary" type="button" id="menu1" data-toggle="dropdown">{{selectYear}}
										<span class="caret"></span>
									</button>
									<ul class="dropdown-menu  scrollable-menu" role="menu" aria-labelledby="menu1"> 
										<li ng-repeat="year in years">
											<a tabindex="-1" href="#" ng-click="setMenuSelection($event, 'Year', year)" >{{year}}</a>
										</li>
									</ul>
								</div>
							</li>

						</li>
						<li>
							<div ng-show="isResultGrid" class="searchresults"> <p class="searchresults">Showing results for      <b><span class="resultTextStyle" ng-bind="results"></span></b></p> </div>
						</li>
						<li>
                          <div ng-show="!isResultGrid"><br/>
					      <p> <i class="fa fa-arrow-left"></i> Select all filters from the menu here to see memory and walltime for your design</p></div>
					    </li>  
				</ul>
			</div> <!-- row height-->
		</div> <!--row-fluid-->
	</div><!-- well div -->
 </div>
 <div class="container-fluid">
 	<div class="well well-md">
		<div class="row-fluid">
			<div class="row-md-height">
				<div class="col-md-2 col-md-height col-padding-overide" >
					<div class="inside inside-full-height">
						<div class="content" ng-include src = "'views/BigBrother/leftNav.html'"></div>
					</div>
				</div> <!-- column -->
				<div class="col-md-10 col-md-height" ng-show="isResultGrid">
					<div class="row">
						<div class="col-md-4 col-padding-overide">
							<i ng-show="!isResultGrid" class="fa fa-5x fa-cog fa-spin"></i>
							<div ng-show="isResultGrid" ui-grid="memoryGrid" ui-grid-selection ui-grid-resize-columns ></div>
							<div ng-hide="dataLoadedMemory" >
                                <div class="bg_load"></div>
                                    <div class="wrapper">
                                        <div class="inner">
                                            <span>L</span>
                                            <span>o</span>
                                            <span>a</span>
                                            <span>d</span>
                                            <span>i</span>
                                            <span>n</span>
                                            <span>g</span>
                                        </div>
                                    </div>        
	                        </div>
                                   <p1>{{noDataGirdMemory}} </p1>
			                
						 </div>
						 
						<div class="col-md-8"  ng-show="isDataMemory">
							<!-- <i ng-show="!isResultMemoryChart" class="fa fa-5x fa-cog fa-spin"></i> -->
							<div ng-show="!isResultMemoryChart"> <p> <i class="fa fa-arrow-left"></i> Select a row on the left to see average, max and min memory. </p> </div>
							<div ng-show="isResultMemoryChart" id="memoryChartBull"></div> 
							<!-- important change this to memoryChartBull -->
							<div ng-show="dataLoadedMemoryChart" >
                                <div class="bg_load_chart">
                                    <div class="wrapper">
                                            <div class="inner">
                                              <span>L</span>
                                              <span>o</span>
                                              <span>a</span>
                                              <span>d</span>
                                              <span>i</span>
                                              <span>n</span>
                                              <span>g</span>
                                            </div>
                                    </div>        
	                        
                                 </div>
                            </div>
						 </div>

                               <div id="" ng-hide="isDataMemory"></div>

					 </div>
					<div class="row">
						<div class="col-md-4 col-padding-overide">
							<i ng-show="!isResultGrid" class="fa fa-5x fa-cog fa-spin"></i>
						   <div ng-show="isResultGrid" ui-grid="walltimeGrid" ui-grid-selection ui-grid-resize-columns></div>
						    <div ng-hide="dataLoadedWalltime" >
						       <div class="bg_load"></div>
                                   <div class="wrapper">
                                       <div class="inner">
                                           <span>L</span>
                                           <span>o</span>
                                           <span>a</span>
                                           <span>d</span>
                                           <span>i</span>
                                           <span>n</span>
                                           <span>g</span>
                                        </div>
                                    </div>        
	                         </div>

	                                  <p1>{{noDataGirdWalltime}} </p1>
	                        
						 </div>

						<div class="col-md-8"  ng-show="isDataWalltime">
							<!-- <i ng-show="!isResultWalltimeChart" class="fa fa-5x fa-cog fa-spin"></i> -->
							<div ng-show="!isResultWalltimeChart"> <p> <i class="fa fa-arrow-left"></i> Select a row on the table to the left to see average, max and min memory. </p> </div>
							<div ng-show="isResultWalltimeChart" id="walltimeChartBull"></div>
							 <!-- important change this to walltimeChartBull -->
							<div ng-show="dataLoadedWalltimeChart" >
                                   <div class="bg_load_chart">
                                       <div class="wrapper">
                                           <div class="inner">
                                               <span>L</span>
                                               <span>o</span>
                                               <span>a</span>
                                               <span>d</span>
                                               <span>i</span>
                                               <span>n</span>
                                               <span>g</span>
                                            </div>
                                        </div> 
                                    </div>       
	                        </div>
						</div>

						       <div id="" ng-hide="isDataWalltime"></div>
				    </div>

				</div> <!-- column -->
				<div class="col-md-10 col-md-height" ng-show="!isResultGrid">
					<p></i></p>
				</div>
			</div> <!-- row height-->
		</div> <!--row-fluid-->
	</div><!-- well div -->
</div> <!--topmost container-fluid div -->


